		ЭтотОбъект.РезультатФН7.ФиксацияСверху = 2;
		ЭтотОбъект.РезультатФН8.ФиксацияСверху = 2;
	КонецЕсли; 	
	
	
	Если ВидОтчета = 0 Тогда		
		КолДопКолонок = 0;
	Иначе
		КолДопКолонок = 1;
	КонецЕсли;   	
	
	ЭтотОбъект.РезультатНадзорныеСоотношения.ФиксацияСверху = 4;
	ЭтотОбъект.РезультатНадзорныеСоотношения.ФиксацияСлева = 4 + КолДопКолонок; 
	
