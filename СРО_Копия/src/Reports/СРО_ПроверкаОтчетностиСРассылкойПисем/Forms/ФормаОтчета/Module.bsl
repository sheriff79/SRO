//&НаСервере
Процедура ПроверкаНаОшибку(КПК, НазваниеТаблицы, НомерСтроки, ЗначениеЛевое, ЗначениеПравое, ЗнакСравнения, ОписаниеУсловия, ОписаниеОшибки, ОписаниеОшибкиГрафа, КоличествоОшибокПоГрафе, СтрокаРазделитель, КодОшибки = "")
	
	Разница = ЗначениеЛевое - ЗначениеПравое;
	Если ?(Разница > 0, Разница, -Разница) > 1 Тогда		
		Если ОписаниеОшибки = "" Тогда
			ОписаниеОшибки = Строка(ЗначениеЛевое) + " " + ЗнакСравнения + " " + Строка(ЗначениеПравое);
		КонецЕсли;
		
		ОписаниеОшибкиГрафа = ОписаниеОшибкиГрафа + ?(ОписаниеОшибкиГрафа="", "", СтрокаРазделитель) + ОписаниеУсловия + "  |  " + ОписаниеОшибки;  
		ДобавитьОшибкуВТаблицу(НомерСтроки, КПК, НазваниеТаблицы, ОписаниеУсловия, ОписаниеОшибки, КодОшибки);         			
		
		КоличествоОшибокПоГрафе = КоличествоОшибокПоГрафе + 1; 
	Иначе
		ОписаниеОшибки = "";
	КонецЕсли; 
	//	
КонецПроцедуры

&НаКлиенте
Процедура ПечатьТаблицыОшибок()
	ТабличныйДокумент = Новый ТабличныйДокумент; 	
	
	// Инициализируем таблицу для последующей обработки
	МассивДеревоКПК = Новый Массив;
	
	ПоказатьТаблицуОшибок(ТабличныйДокумент,, МассивДеревоКПК);
	
	ПараметрыДляПередачи = Новый Структура;
	ПараметрыДляПередачи.Вставить("ТабличныйДокумент", ТабличныйДокумент);
	ПараметрыДляПередачи.Вставить("МассивДеревоКПК", МассивДеревоКПК);
	
	//ФормаОшибок = ПолучитьФорму("ВнешнийОтчет.ПроверкаОтчетностиСРассылкойПисем.Форма.ФормаОписанияОшибок", ПараметрыДляПередачи, ЭтаФорма);
	ФормаОшибок = ПолучитьФорму("Отчет.СРО_ПроверкаОтчетностиСРассылкойПисем.Форма.ФормаОписанияОшибок", ПараметрыДляПередачи, ЭтаФорма);
	ФормаОшибок.Открыть();
	
	
