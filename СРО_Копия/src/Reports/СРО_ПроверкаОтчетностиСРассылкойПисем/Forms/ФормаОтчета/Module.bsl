&НаСервере
Функция ПолучитьРезультатТаблицыОшибокСОтборами(КПК, НаименованиеТаблицы) 	
	
	Запрос = Новый Запрос;
	ТекстЗапроса = 
	"ВЫБРАТЬ
	|	ТаблицаОшибок.КПК КАК КПК,
	|	ТаблицаОшибок.НазваниеТаблицы КАК НазваниеТаблицы,
	|	ТаблицаОшибок.ОписаниеОшибки КАК ОписаниеОшибки,
	|	ТаблицаОшибок.ОписаниеУсловия КАК ОписаниеУсловия,
	|	ТаблицаОшибок.НомерСтроки КАК НомерСтроки,
	|	ТаблицаОшибок.КоличествоОшибок КАК КоличествоОшибок,
	|	ТаблицаОшибок.КодОшибки КАК КодОшибки,   
	|	ТаблицаОшибок.ПорядокСортировки КАК ПорядокСортировки
	|ПОМЕСТИТЬ ВТ
	|ИЗ
	|	&ТаблицаОшибок КАК ТаблицаОшибок
	|ГДЕ
	|	Истина
	|	#УсловияОтбора#
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ВТ.КПК КАК КПК,
	|	ВТ.НазваниеТаблицы КАК НазваниеТаблицы,
	|	ВТ.ОписаниеОшибки КАК ОписаниеОшибки,
	|	ВТ.ОписаниеУсловия КАК ОписаниеУсловия,
	|	ВТ.НомерСтроки КАК НомерСтроки,
	|	СУММА(ВТ.КоличествоОшибок) КАК КоличествоОшибок,
	|	ВТ.КодОшибки КАК КодОшибки,
	|	ВТ.ПорядокСортировки КАК ПорядокСортировки
	|ИЗ
	|	ВТ КАК ВТ
	|
	|СГРУППИРОВАТЬ ПО
	|	ВТ.КПК,
	|	ВТ.НазваниеТаблицы,
	|	ВТ.ОписаниеОшибки,
	|	ВТ.ОписаниеУсловия,
	|	ВТ.НомерСтроки,
	|	ВТ.КодОшибки,
	|	ВТ.ПорядокСортировки
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтроки,
	|	ПорядокСортировки
	|ИТОГИ
	|	СУММА(КоличествоОшибок)
	|ПО
	|	КПК,
	|	НазваниеТаблицы";  	
	
	
	Запрос.УстановитьПараметр("ТаблицаОшибок", Отчет.ТаблицаОшибок.Выгрузить());
	
	УсловияОтбора = "";       
	Если КПК <> Неопределено Тогда
		УсловияОтбора = УсловияОтбора + " И ТаблицаОшибок.КПК = &КПК";
		Запрос.УстановитьПараметр("КПК", КПК);
	КонецЕсли;
	
	Если НаименованиеТаблицы <> Неопределено Тогда
		УсловияОтбора = УсловияОтбора + " И ТаблицаОшибок.НазваниеТаблицы = &НазваниеТаблицы";		
		Запрос.УстановитьПараметр("НазваниеТаблицы", НаименованиеТаблицы);
	КонецЕсли;
	
	ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "#УсловияОтбора#", УсловияОтбора); 
	
	Запрос.Текст = ТекстЗапроса;
	
	РезультатЗапроса = Запрос.Выполнить();
	
	Если РезультатЗапроса.Пустой() Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	Возврат РезультатЗапроса; 	
	
КонецФункции


&НаСервере
Процедура ПоказатьТаблицуОшибок(ТабличныйДокумент, ПараметрыОтчета = Неопределено, МассивДеревоКПК = Неопределено)
	
	Если ПараметрыОтчета = Неопределено Тогда
		КПК = Неопределено;
		НаименованиеТаблицы = Неопределено;
	Иначе
		КПК = ПараметрыОтчета.ДокументОтчетности.КПК;
		НаименованиеТаблицы = ПараметрыОтчета.НаименованиеТаблицы;
	КонецЕсли;
	
	ОтчетОбъект = РеквизитФормыВЗначение("Отчет");
	
	Макет = ОтчетОбъект.ПолучитьМакет("РасшифровкаОшибков");
	
	Если ВидОтчета = 0 Тогда
		НачалоПериода = НачалоКвартала(ДатаОкончанияОтчетногоПериода);
		ОкончаниеПериода = КонецКвартала(ДатаОкончанияОтчетногоПериода);
	Иначе
		НачалоПериода = НачалоГода(ДатаОкончанияОтчетногоПериода);
		ОкончаниеПериода = КонецГода(ДатаОкончанияОтчетногоПериода);
	КонецЕсли;
	
	
	ОбластьШапка = Макет.ПолучитьОбласть("Шапка");
	ОбластьКПК = Макет.ПолучитьОбласть("КПК");
	ОбластьНазваниеТаблицы = Макет.ПолучитьОбласть("НазваниеТаблицы");
	ОбластьСтрока = Макет.ПолучитьОбласть("Строка");
	
	//Если СтруктураДеревоКПК <> Неопределено Тогда	
	//	СтруктураДеревоКПК = Новый ДеревоЗначений;
	//	СтруктураДеревоКПК.Колонки.Добавить("КПК");
	//	СтруктураДеревоКПК.Колонки.Добавить("Раздел"); 	
	//КонецЕсли;
	
	
	ОбластьШапка.Параметры.ПредставлениеПериода =  ПредставлениеПериода(НачалоПериода, ОкончаниеПериода);
	
	ОбластьШапка.Параметры.ОтборПоКПК = ?(ЗначениеЗаполнено(КПК), "Отбор по " + СокрЛП(КПК.Наименование), "Без отбора по КПК");	       
	ТабличныйДокумент.Вывести(ОбластьШапка);
	
	// Выводим результаты запроса
	ТабличныйДокумент.НачатьАвтогруппировкуСтрок();
	
	РезультатЗапроса =  ПолучитьРезультатТаблицыОшибокСОтборами(КПК, НаименованиеТаблицы); 
	
	Если РезультатЗапроса <> Неопределено Тогда       		
		
		// По КПК    	
		ВыборкаПоКПК = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		
		Пока ВыборкаПоКПК.Следующий() Цикл               		
			ОбластьКПК.Параметры.НомерСтроки	    = ВыборкаПоКПК.НомерСтроки;
			ОбластьКПК.Параметры.НомерВРеестреСРО   = ВыборкаПоКПК.КПК.НомерВРеестреСРО;   				
			ОбластьКПК.Параметры.КраткоеНазваниеКПК = СокрЛП(ВыборкаПоКПК.КПК.НаименованиеКраткое);	
			ОбластьКПК.Параметры.КоличествоОшибок = СокрЛП(ВыборкаПоКПК.КоличествоОшибок);	
			ТабличныйДокумент.Вывести(ОбластьКПК, 1);
			
			ВыборкаПоТаблицам = ВыборкаПоКПК.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
			
			// Дерево  
			
			
			МассивРазделовОшибок = Новый Массив;
			Пока ВыборкаПоТаблицам.Следующий() Цикл
				НазваниеТаблицы = СокрЛП(ВыборкаПоТаблицам.НазваниеТаблицы);
				
				ОбластьНазваниеТаблицы.Параметры.НазваниеТаблицы = НазваниеТаблицы;	
				ОбластьНазваниеТаблицы.Параметры.КоличествоОшибок = СокрЛП(ВыборкаПоТаблицам.КоличествоОшибок);	
				ТабличныйДокумент.Вывести(ОбластьНазваниеТаблицы, 2); 
				
				// Дерево  
				Если МассивДеревоКПК <> Неопределено Тогда
					МассивРазделовОшибок.Добавить(НазваниеТаблицы);
					
					//СтрокаДереваРаздел = СтрокаДереваКПК.Строки.Добавить();
					//СтрокаДереваРаздел.Раздел = СокрЛП(ВыборкаПоТаблицам.НазваниеТаблицы); 			
				КонецЕсли;
				
				Выборка =  ВыборкаПоТаблицам.Выбрать();
				
				Пока Выборка.Следующий() Цикл 			
					ОбластьСтрока.Параметры.ОписаниеУсловия	   = Выборка.ОписаниеУсловия;
					ОбластьСтрока.Параметры.ОписаниеОшибки     = Выборка.ОписаниеОшибки; 
					ОбластьСтрока.Параметры.КодОшибки     = Выборка.КодОшибки; 
					
					ТабличныйДокумент.Вывести(ОбластьСтрока, 3);
				Конеццикла;
			КонецЦикла;
			
			// Дерево 			
			Если МассивРазделовОшибок.Количество() > 0 Тогда
				
				МассивДанныхКПКРазделы = Новый Структура;
				МассивДанныхКПКРазделы.Вставить("КПК", ВыборкаПоКПК.КПК);
				МассивДанныхКПКРазделы.Вставить("МассивРазделовОшибок", МассивРазделовОшибок);
				
				МассивДеревоКПК.Добавить(МассивДанныхКПКРазделы);
			КонецЕсли;
			
			
		КонецЦикла;
	КонецЕсли;
	
	ТабличныйДокумент.ЗакончитьАвтогруппировкуСтрок();  		
	
КонецПроцедуры


