	КонецЕсли;		
	
	Если СтруктураТабличныхДокументов.Свойство("ТабДокФН5") Тогда
		РезультатФН5 = СтруктураТабличныхДокументов.ТабДокФН5;
	КонецЕсли;		
	
	Если СтруктураТабличныхДокументов.Свойство("ТаблицаФН6") Тогда
		РезультатФН6 = СтруктураТабличныхДокументов.ТаблицаФН6;
	КонецЕсли;		
	
	Если СтруктураТабличныхДокументов.Свойство("ТабДокФН7") Тогда
		РезультатФН7 = СтруктураТабличныхДокументов.ТабДокФН7;
	КонецЕсли;		
	
	Если СтруктураТабличныхДокументов.Свойство("ТабДокФН8") Тогда
		РезультатФН8 = СтруктураТабличныхДокументов.ТабДокФН8;
	КонецЕсли;	
	
	Если СтруктураТабличныхДокументов.Свойство("ТабДокФН_Расшифровка") Тогда
		РезультатФН_Расшифровка = СтруктураТабличныхДокументов.ТабДокФН_Расшифровка;
	КонецЕсли;	 	
	
	РезультатФН1.ФиксацияСверху = 2;
	РезультатФН2.ФиксацияСверху = 2;
	РезультатФН3.ФиксацияСверху = 2;
	РезультатФН4.ФиксацияСверху = 2;
	РезультатФН5.ФиксацияСверху = 2;
	РезультатФН6.ФиксацияСверху = 2;
	РезультатФН7.ФиксацияСверху = 2;
	РезультатФН8.ФиксацияСверху = 2;
	//	КонецЕсли; 	
	
	
	Если ВидОтчета = 0 Тогда		
		КолДопКолонок = 0;
	Иначе
		КолДопКолонок = 1;
	КонецЕсли;   	
	
	РезультатНадзорныеСоотношения.ФиксацияСверху = 4;
	РезультатНадзорныеСоотношения.ФиксацияСлева = 4 + КолДопКолонок; 
	
	РезультатКС = СтруктураТабличныхДокументов.РезультатКС;
	РезультатНормативы = СтруктураТабличныхДокументов.РезультатНормативы; 	 	
	
	ЗначениеВРеквизитФормы(ОтчетОбъект, "Отчет");
	
КонецПроцедуры

&НаСервере 
Процедура СформироватьНаСервере(ТабличныйДокумент)
	
	Отчет.ТаблицаОшибок.Очистить(); 
	
	РезультатНадзорныеСоотношения.Очистить();
	РезультатКС.Очистить();
	РезультатНормативы.Очистить();
	
	
	НеВыводим_ОКУД0420820 = Ложь;
	
	Если НачалоГода(ПериодОтчета.ДатаОкончания) = НачалоКвартала(ПериодОтчета.ДатаОкончания) Тогда		
		// 1 квартала не выводим эту ппроверку
		НеВыводим_ОКУД0420820 = Истина;
	КонецЕсли;	
	
	МассивКПК = Новый Массив;
	
	СтруктураПараметров = Новый Структура;
	СтруктураПараметров.Вставить("МассивКПК", ?(ВидОтчета = 0, МассивКПК, СписокКПК.ВыгрузитьЗначения()));
	СтруктураПараметров.Вставить("ДатаНачалаОтчета", ПериодОтчета.ДатаНачала);
	СтруктураПараметров.Вставить("ДатаОкончанияОтчета", КонецКвартала(ПериодОтчета.ДатаОкончания)); 	
	СтруктураПараметров.Вставить("ОтчетностьСРО", Отчет.ОтчетностьСРО); 	
	СтруктураПараметров.Вставить("ПредварительнаяПроверка", Отчет.ПредварительнаяПроверка); 
	СтруктураПараметров.Вставить("ВидОтчета", ВидОтчета); 
	СтруктураПараметров.Вставить("ВыводитьТолькоКПКСОтчетностью", ВыводитьТолькоКПКСОтчетностью);     	
	СтруктураПараметров.Вставить("ТаблицаОшибок", Отчет.ТаблицаОшибок.Выгрузить());  	
	СтруктураПараметров.Вставить("ОтчетВЦБ", ОтчетВЦБ); 
	СтруктураПараметров.Вставить("ФормированиеОтчетаПользователю", Ложь); 
	
	СтруктураПараметров.Вставить("ТипОтчета",Перечисления.ТипыОчетаПроверки.ПоРазделам);			
//	СтруктураПараметров.Вставить("ВидОтчета", ВидОтчета);  	
	
	ВремСтруктураТабличныхДокументов = Отчеты.СРО_ПроверкаОтчетностиСРассылкойПисем.СформироватьТабличныеДокументы(СТруктураПараметров);

	Если ТипЗнч(ВремСтруктураТабличныхДокументов) = Тип("Структура") Тогда
		
		СтруктураТабличныхДокументов = ВремСтруктураТабличныхДокументов;      		
		ВывестиТаблицы(СтруктураТабличныхДокументов, СтруктураТабличныхДокументов);       		
		
	ИначеЕсли ТипЗнч(ВремСтруктураТабличныхДокументов) = Тип("Массив") Тогда
		
		СтруктураТабличныхДокументов = ВремСтруктураТабличныхДокументов[0];
		ВывестиТаблицы(СтруктураТабличныхДокументов, СтруктураТабличныхДокументов);
		
	КонецЕсли; 	
	
КонецПроцедуры


