&НаСервере
Процедура ДобавитьОшибкуВТаблицу(ТаблицаОшибок, НомерСтроки, КПК, НазваниеТаблицы, ОписаниеУсловия, ОписаниеОшибки, КодОшибки = "")
	
	НоваяСтрока = ТаблицаОшибок.Добавить();
	НоваяСтрока.НомерСтроки = НомерСтроки;
	НоваяСтрока.НомерВРеестреСРО = КПК.НомерВРеестреСРО;  
	НоваяСтрока.КПК = КПК;
	НоваяСтрока.НазваниеТаблицы = НазваниеТаблицы;
	НоваяСтрока.ОписаниеУсловия = ОписаниеУсловия;
	НоваяСтрока.ОписаниеОшибки = ОписаниеОшибки; 
	НоваяСтрока.КоличествоОшибок = 1;
	НоваяСтрока.ПорядокСортировки = ТаблицаОшибок.Количество();
	НоваяСтрока.КодОшибки = КодОшибки;
	
КонецПроцедуры

//&НаСервере
Процедура ПроверкаНаОшибку(ТаблицаОшибок, КПК, НазваниеТаблицы, НомерСтроки, ЗначениеЛевое, ЗначениеПравое, ЗнакСравнения, ОписаниеУсловия, ОписаниеОшибки, ОписаниеОшибкиГрафа, КоличествоОшибокПоГрафе, СтрокаРазделитель, КодОшибки = "")
	
	Разница = ЗначениеЛевое - ЗначениеПравое;
	Если ?(Разница > 0, Разница, -Разница) > 1 Тогда		
		Если ОписаниеОшибки = "" Тогда
			ОписаниеОшибки = Строка(ЗначениеЛевое) + " " + ЗнакСравнения + " " + Строка(ЗначениеПравое);
		КонецЕсли;
		
		ОписаниеОшибкиГрафа = ОписаниеОшибкиГрафа + ?(ОписаниеОшибкиГрафа="", "", СтрокаРазделитель) + ОписаниеУсловия + "  |  " + ОписаниеОшибки;  
		ДобавитьОшибкуВТаблицу(ТаблицаОшибок, НомерСтроки, КПК, НазваниеТаблицы, ОписаниеУсловия, ОписаниеОшибки, КодОшибки);         			
		
		КоличествоОшибокПоГрафе = КоличествоОшибокПоГрафе + 1; 
	Иначе
		ОписаниеОшибки = "";
	КонецЕсли; 
	//	
КонецПроцедуры


